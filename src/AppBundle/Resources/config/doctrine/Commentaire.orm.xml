<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Commentaire" table="COMMENTAIRE">
    <id name="idcommentaire" type="integer" column="idCommentaire">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="ressourceId" type="integer" column="ressource_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="ressourceType" type="string" column="ressource_type" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dateajout" type="datetime" column="dateAjout" nullable="false"/>
    <field name="pseudo" type="string" column="pseudo" length="30" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="255" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="commentaire" type="string" column="commentaire" length="200" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
