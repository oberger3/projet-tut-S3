<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Artiste" table="ARTISTE">
    <id name="idartiste" type="integer" column="idArtiste">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nomartiste" type="string" column="nomArtiste" length="25" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="prenomartiste" type="string" column="prenomArtiste" length="25" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="domaine" type="string" column="domaine" length="25" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
